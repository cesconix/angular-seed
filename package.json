{
  "private": true,
  "name": "angular-seed",
  "license": "See LICENSE file",
  "version": "0.0.1",
  "description": "Angular Seed",
  "author": "Francesco Pasqua <francesco.pasqua@h-farm.com>",
  "scripts": {
    "serve": "cross-env API_SERVER=production webpack-dev-server",
    "serve:dev": "concurrently --kill-others \"cross-env API_SERVER=development webpack-dev-server\" \"npm run mock\"",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "build:dev": "cross-env NODE_ENV=development webpack --progress",
    "prepublish": "npm run clean && npm run build && cp src/index.html dist",
    "mock": "apimocker -c mock/config.json",
    "clean": "rimraf dist coverage docs",
    "test": "karma start --single-run --reporters spec",
    "test:watch": "npm test -- --no-single-run --auto-watch --reporters spec",
    "test:cov": "npm test -- --reporters coverage",
    "docs": "esdoc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cesconix/angular-seed.git"
  },
  "devDependencies": {
    "apimocker": "0.5.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "bundle-loader": "0.5.5",
    "concurrently": "3.4.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "3.1.4",
    "css-loader": "0.26.2",
    "esdoc": "^0.5.2",
    "eslint": "3.16.1",
    "eslint-config-airbnb": "14.1.0",
    "eslint-config-angular": "0.5.0",
    "eslint-loader": "1.6.3",
    "eslint-plugin-angular": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "file-loader": "0.10.1",
    "jasmine-core": "2.5.2",
    "json-loader": "0.5.4",
    "karma": "1.5.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.30",
    "karma-webpack": "2.0.2",
    "ng-annotate-loader": "0.2.0",
    "node-sass": "4.5.0",
    "postcss-loader": "1.3.3",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.2",
    "style-loader": "0.13.2",
    "uglify-loader": "1.4.0",
    "url-loader": "0.5.8",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1"
  },
  "dependencies": {
    "angular": "1.5.10",
    "angular-mocks": "1.5.10",
    "angular-resource": "1.5.10",
    "angular-ui-router": "1.0.0-rc.1",
    "autoprefixer": "6.7.6"
  }
}
